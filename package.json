{
  "name": "otemae",
  "description": "Otomae",
  "private": true,
  "scripts": {
    "commit": "git-cz",
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "start": "turbo start --no-daemon --no-cache",
    "test": "turbo test",
    "build": "turbo build --no-daemon --no-cache",
    "dev": "turbo dev --parallel --continue --no-daemon --no-cache"
  },
  "workspaces": [
    "apps/**",
    "libs/**"
  ],
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "3.3.0",
    "husky": "^8.0.2",
    "prettier": "^2.7.1",
    "turbo": "^1.6.3"
  },
  "dependencies": {},
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "license": "MIT",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:nathan2slime/spacehunter.git",
  "author": "nathan3boy <nathan3boss@gmail.com>"
}
